{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "@/*": [
                "*"
            ]
        },
        "strict": true,
        "jsx": "react-jsx",
        "jsxImportSource": "hono/jsx",
        "skipLibCheck": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noPropertyAccessFromIndexSignature": true,
        "downlevelIteration": true
    }
}
